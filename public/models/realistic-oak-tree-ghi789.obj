# Realistic Oak Tree - Test Model
# Simple tree shape for testing

# Trunk vertices
v -0.3 0.0 -0.3    # trunk base left back
v  0.3 0.0 -0.3    # trunk base right back
v  0.3 0.0  0.3    # trunk base right front
v -0.3 0.0  0.3    # trunk base left front
v -0.2 3.0 -0.2    # trunk top left back
v  0.2 3.0 -0.2    # trunk top right back
v  0.2 3.0  0.2    # trunk top right front
v -0.2 3.0  0.2    # trunk top left front

# Tree canopy vertices (sphere-like)
v -2.0 3.0 -2.0    # canopy bottom left back
v  2.0 3.0 -2.0    # canopy bottom right back
v  2.0 3.0  2.0    # canopy bottom right front
v -2.0 3.0  2.0    # canopy bottom left front
v -1.5 5.0 -1.5    # canopy middle left back
v  1.5 5.0 -1.5    # canopy middle right back
v  1.5 5.0  1.5    # canopy middle right front
v -1.5 5.0  1.5    # canopy middle left front
v  0.0 6.0  0.0    # canopy top center

# Texture coordinates
vt 0.0 0.0
vt 1.0 0.0
vt 1.0 1.0
vt 0.0 1.0
vt 0.5 0.5

# Vertex normals
vn 0.0  1.0 0.0    # up
vn 0.0 -1.0 0.0    # down
vn 0.0  0.0 -1.0   # back
vn 0.0  0.0  1.0   # front
vn -1.0 0.0 0.0    # left
vn  1.0 0.0 0.0    # right

# Faces
# Trunk (simplified box)
f 1/1/2 2/2/2 3/3/2 4/4/2
f 8/1/1 7/2/1 6/3/1 5/4/1
f 1/1/3 5/2/3 6/3/3 2/4/3
f 4/1/4 3/2/4 7/3/4 8/4/4
f 1/1/5 4/2/5 8/3/5 5/4/5
f 2/1/6 6/2/6 7/3/6 3/4/6

# Tree canopy (simplified)
f 9/1/2 10/2/2 11/3/2 12/4/2
f 13/1/1 14/2/1 15/3/1 16/4/1
f 17/5/1 13/1/1 14/2/1
f 17/5/1 14/2/1 15/3/1
f 17/5/1 15/3/1 16/4/1
f 17/5/1 16/4/1 13/1/1
EOF < /dev/null